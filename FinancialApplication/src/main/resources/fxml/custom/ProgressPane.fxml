<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<BorderPane fx:id="progressPane" visible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="com.sjm.financialapplication.component.ProgressPaneController" >
    <top>
        <Label fx:id="messageLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" textFill="#0066ff" BorderPane.alignment="CENTER">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
    </top>
    <center>
        <ProgressIndicator fx:id="progressIndicator" maxHeight="100.0" maxWidth="100.0" progress="0.0" />
    </center>
</BorderPane>
